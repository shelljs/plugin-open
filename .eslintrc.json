{
  "env": {
    "node": true
  },
  "extends": "airbnb-base",
  "rules": {
    "import/no-mutable-exports": 0,
    "global-require": 0,
    "vars-on-top": 0,
    "spaced-comment": [2, "always", { "markers": ["@", "@include"], "exceptions": ["@"] }],
    "no-param-reassign": 0,
    "no-console": 0,
    "curly": 0,
    "no-var": 0,
    "prefer-arrow-callback": 0,
    "prefer-template": 0,
    "func-names": 0,
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "import/order": 0,
    "no-restricted-globals": 0,
    "new-cap": [2, {
      "capIsNewExceptions": [
        "ShellString"
      ]}
    ]
  }
}
